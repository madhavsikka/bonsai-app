{
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "decorations": false,
        "fullscreen": true,
        "title": "bonsai"
      }
    ]
  },
  "build": {
    "beforeBuildCommand": "npm run build",
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../dist"
  },
  "bundle": {
    "active": true,
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "targets": "all"
  },
  "identifier": "bons.ai",
  "plugins": {
    "shell": {
      "open": true
    }
  },
  "productName": "bonsai",
  "version": "0.0.0"
}