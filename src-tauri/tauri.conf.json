{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:5500"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "bonsai",
  "version": "0.0.1",
  "identifier": "bons.ai",
  "plugins": {},
  "app": {
    "windows": [
      {
        "title": "bonsai",
        "resizable": true,
        "width": 1024,
        "height": 768
      }
    ],
    "security": {
      "assetProtocol": {
        "scope": [
          "*/**"
        ],
        "enable": true
      },
      "csp": "default-src *; img-src * asset: https://asset.localhost;; connect-src ipc: http://ipc.localhost"
    }
  }
}